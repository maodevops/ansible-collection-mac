---
# Copy this file to vars/user.yml and customize.
# vars/user.yml is gitignored so your personal settings won't be committed.

# Add extra Homebrew packages on top of the team baseline:
brew_packages_extra: []

# Add extra Homebrew casks:
brew_casks_extra:
  - obsidian

# Add extra global gitignore patterns:
git_global_ignore_extra: []

# Set your git identity:
git_user_name: "Your Name"
git_user_email: "you@example.com"

# Path to a directory containing your iTerm2 preferences (e.g. from a dotfiles repo):
# iterm2_prefs_dir: "~/dotfiles/iterm2"
